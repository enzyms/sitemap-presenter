{
  "name": "sitemap-presenter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:web": "pnpm --filter @sitemap-presenter/web dev",
    "dev:server": "pnpm --filter @sitemap-presenter/server dev",
    "dev:widget": "pnpm --filter @sitemap-presenter/widget dev",
    "build": "pnpm -r build",
    "build:widget": "pnpm --filter @sitemap-presenter/widget build",
    "lint": "pnpm -r lint"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
